<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="This page is not ready yet"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GO BACK TO THE VOLUNTEER MENU"
        android:id="@+id/btnMoveToVolunteerMenu"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GO TO LIST OF VOLUNTEERS"
        android:id="@+id/btnMoveToVolunteerList"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GO TO LIST OF Groups"
        android:id="@+id/btnMoveToGroupList"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GO TO Adding Menu"
        android:id="@+id/btnMoveToAddMenu"/>
</LinearLayout>